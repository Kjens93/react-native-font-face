{
  "name": "react-native-font-faces",
  "version": "0.0.0",
  "description": "Easily emulate @font-face behavior in react-native",
  "repository": "https://github.com/Kjens93/react-native-font-faces.git",
  "author": "Kyler Jensen <kjens93@users.noreply.github.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "src/index.ts",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "clean": "rm -rf dist",
    "build": "yarn clean && yarn build-typings && yarn build-sources",
    "build-typings": "tsc -outDir dist -p ./tsconfig.build.json",
    "build-sources": "babel src -x '.ts,.tsx' --out-dir dist --config-file ./babel.config.build.js",
    "prepublishOnly": "yarn build"
  },
  "peerDependencies": {
    "react": ">=16.11.0",
    "react-native": ">=0.62.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@types/jest": "^26.0.4",
    "@types/react": "^16.9.43",
    "@types/react-native": "^0.62.0",
    "babel-jest": "^26.1.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "metro-react-native-babel-preset": "^0.60.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "typescript": "^3.9.6"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  }
}
